  <div class="container">
    <header>
      <h1>SINAU HTML, CSS & Ruby on Rails with SIMBAH -odet</h1>
      <h2>Logino nganggo akunmu seng bener, rung ndue?
          <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
          <%= link_to "daftar", new_registration_path(resource_name) %>
          <% end %>
      </h2>
    </header>
      <div class="badan">
        <ul>
          <li><a href=""><img src="/assets/ico.jpg"></a></li>
          <li>Login</li>
          <li>
      
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

            <div class="field">
              <%= f.label :email,class: "label" %>
              <%= f.email_field :email, autofocus: true, autocomplete: "email",class: "kolom" %>
            </div>
            
            <div class="field">
              <%= f.label :password,class: "label" %>
              <%= f.password_field :password, autocomplete: "current-password",class: "kolom" %>
            </div>

            <% if devise_mapping.rememberable? %>

            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
            </div>
            
            <% end %>

            <div class="actions">
              <%= f.submit "Log in" %>
            </div>
            
            <% end %>
            <%= render "devise/shared/links" %>

          </li>
        </ul>
      </div>
  </div>